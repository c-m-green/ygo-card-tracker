                        <TableColumn prefWidth="75.0" text="Variant" />
                        <TableColumn prefWidth="75.0" text="Attribute" />
                        <TableColumn prefWidth="75.0" text="Scale" />
                        <TableColumn prefWidth="75.0" text="Link Value" />
                        <TableColumn prefWidth="75.0" text="Link Markers" />
                        <TableColumn prefWidth="75.0" text="Image Link" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</Group>
