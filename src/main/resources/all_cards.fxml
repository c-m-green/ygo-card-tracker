<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<Group xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cgreen.ygocardtracker.menu.AllCardsController">
   <children>
      <VBox prefHeight="513.0" prefWidth="1006.0">
         <children>
            <TableView fx:id="tableView" prefHeight="536.0" prefWidth="1006.0">
              <columns>
                <TableColumn fx:id="cardInfoIdCol" editable="false" prefWidth="96.0" text="card_info_id" />
                <TableColumn fx:id="deckIdCol" editable="false" minWidth="0.0" prefWidth="75.0" text="deck_id" />
                  <TableColumn fx:id="setCodeCol" editable="false" prefWidth="75.0" text="set_code" />
                  <TableColumn fx:id="isInSideDeckCol" editable="false" prefWidth="75.0" text="in_side_deck" />
                  <TableColumn fx:id="isVirtualCol" prefWidth="75.0" text="is_virtual" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#handleDeleteButtonAction" text="Delete" />
              </buttons>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</Group>
