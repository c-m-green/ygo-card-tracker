<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<Group xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cgreen.ygocardtracker.menu.AllCardsController">
   <children>
      <VBox prefHeight="513.0" prefWidth="1006.0">
         <children>
            <TableView fx:id="tableView" prefHeight="536.0" prefWidth="1006.0">
              <columns>
                <TableColumn fx:id="passcodeCol" editable="false" prefWidth="96.0" text="Passcode" />
                <TableColumn fx:id="nameCol" editable="false" minWidth="0.0" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="descCol" editable="false" prefWidth="75.0" text="Description" />
                  <TableColumn fx:id="cardTypeCol" editable="false" prefWidth="75.0" text="Type" />
                  <TableColumn fx:id="variantCol" prefWidth="75.0" text="Variant" />
                  <TableColumn fx:id="attributeCol" prefWidth="75.0" text="Attribute" />
                  <TableColumn fx:id="atkCol" prefWidth="75.0" text="ATK" />
                  <TableColumn fx:id="defCol" prefWidth="75.0" text="DEF" />
                  <TableColumn fx:id="levelCol" prefWidth="75.0" text="Level" />
                  <TableColumn fx:id="scaleCol" prefWidth="75.0" text="Scale" />
                  <TableColumn fx:id="linkValCol" prefWidth="75.0" text="Link Rating" />
                  <TableColumn fx:id="linkMarkersCol" prefWidth="75.0" text="Link Markers" />
                  <TableColumn fx:id="imageCol" prefWidth="75.0" text="Image Link" />
                  <TableColumn fx:id="imageSmallCol" prefWidth="75.0" text="Image Link (small)" />
                  <TableColumn fx:id="isFakeCol" prefWidth="75.0" text="is_fake" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button fx:id="deleteButton" onAction="#handleDeleteButtonAction" mnemonicParsing="false" text="Delete" />
              </buttons>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</Group>
